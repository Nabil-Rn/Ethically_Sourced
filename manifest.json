{
      "manifest_version": 2,
      "version": "1.0",
      "name": "Ethically Sourced",
      "description": "Bell Geekfest Hackathon",
      "permissions": [
            "tabs",
            "*://safebrowsing.googleapis.com/*"
      ],
      "icons": {
            "16": "ethicallysourcedlogo.svg",
            "32": "ethicallysourcedlogo.svg",
            "48": "ethicallysourcedlogo.svg",
            "128": "ethicallysourcedlogo.svg"
          },
      "content_scripts": [
            {
                  "matches": [
                        "<all_urls>"
                  ],
                  "js": [
                        "main.js"
                  ]
            }
      ],
      "browser_action": {
            "default_popup": "index.html",
            "default_icon": {
                  "19": "ethicallysourcedlogo.svg",
                  "38": "ethicallysourcedlogo.svg"
            }
      },
      "background": {
            "scripts": [
                  "background.js"
            ]
      }
}